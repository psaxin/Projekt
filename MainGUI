    protected void fyllDoman(){
        String sqlFraga = "Select BENAMNING FROM KOMPETENSDOMAN";
        cbDoman.removeAllItems();
        cbDomDomPlatt.removeAllItems();
        
        try{
            ArrayList<String> domaner = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla domäner");
            for(int i = 0; i<domaner.size();i++){
                cbDoman.addItem(domaner.get(i));
                cbDomDomPlatt.addItem(domaner.get(i));
            }
        }
        
        catch (InfException e){
            System.out.println("Det fungerade inte att lägga till domäner");
        }
    }
    protected void fyllPlattform(){
        String sqlFraga = "SELECT BENAMNING FROM PLATTFORM";
        cbUppdProjPlattPlatt.removeAllItems();
        cbPlattDomPlatt.removeAllItems();
        
        try{
            
            ArrayList<String> plattformar = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla på plattformar");
            for(int i =0;i<plattformar.size();i++)
            {
                cbUppdProjPlattPlatt.addItem(plattformar.get(i));
                cbPlattDomPlatt.addItem(plattformar.get(i));
            }
        }
        
        catch(InfException e)
        {
            System.out.println("Fungerade EJ");
            
        }
    }
    protected void fyllcbInfoProj(){
        
        String sqlFraga = "SELECT BETECKNING FROM SPELPROJEKT";
        cbInfoProj.removeAllItems();
        
        try{
            ArrayList<String> projekt  = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla på cbInfoProj");
            for(int i =0;i<projekt.size();i++)
            {
                cbInfoProj.addItem(projekt.get(i));
                
            }
        }
        catch (InfException e)
        {
            System.out.println("Fungerar ej att fylla på cbInfo");
            
        }
        
    }
    
    protected String andraDatum(String datum){
        
        String aa =datum.substring(0,4);
        String dd =datum.substring(5,7);
        String mm =datum.substring(8);
        String omvandlat = dd+"-"+mm+"-"+aa;
        
        return omvandlat;
        
        
    }
    
    
    public static void main(String args[]) {
