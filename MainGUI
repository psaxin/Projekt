    private void bAnstalldUppdateraActionPerformed(java.awt.event.ActionEvent evt) {                                                   
        String anstalldNamn = cbUppdAnstalld.getSelectedItem().toString();
        String nyMail = tbNyMail.getText();
        String nyTlf = tbNyTlf.getText();

        String sqlFragaMail = "Update Anstalld set MAIL = '"+nyMail+"' where NAMN = '"+anstalldNamn+"'";
        String sqlFragaTlf ="Update Anstalld set TELEFON = '"+nyTlf+"' where NAMN = '"+anstalldNamn+"'";

        try{
            idb.update(sqlFragaMail);
            System.out.println("Det funkade " + sqlFragaMail);
        }

        catch (InfException e){
            System.out.println("Det blev fel " + sqlFragaMail);
        }

        try{
            idb.update(sqlFragaTlf);
            System.out.println("Det funkade " + sqlFragaTlf);
        }

        catch (InfException e){
            System.out.println("Det blev fel " + sqlFragaTlf);
        }

        tbgamAID.setText("");
        tbGamNamn.setText("");
        tbGamTlf.setText("");
        tbGamMail.setText("");
        tbgamAnv.setText("");
        tbAID.setText("");
        tbNamn.setText("");
        tbAnv.setText("");
        tbNyMail.setText("");
        tbNyTlf.setText("");

    }                                                  

    private void cbUppdAnstalldPopupMenuWillBecomeInvisible(javax.swing.event.PopupMenuEvent evt) {                                                            
        String namn = cbUppdAnstalld.getSelectedItem().toString();
