        String tlf = getTlfAnst();
        String mail = getMailAnst();
        String anvNamn = getAnvNamnAnst();

        tbgamAID.setText(aID);
        tbGamNamn.setText(namn);
        tbGamTlf.setText(tlf);
        tbGamMail.setText(mail);
        tbgamAnv.setText(anvNamn);
        tbAID.setText(aID);
        tbNamn.setText(namn);
        tbAnv.setText(anvNamn);

    }                                                           

    private void bTaBortAnstalldActionPerformed(java.awt.event.ActionEvent evt) {                                                
        String anstalldNamn = cbTaBortAnstalld.getSelectedItem().toString();
        String sqlFraga = "Delete from Anstalld where namn = '"+anstalldNamn+"'";

        try{
            idb.delete(sqlFraga);
            System.out.println("Det funkade " + sqlFraga);
        }

        catch (InfException e){
            System.out.println("Funkade inte " + sqlFraga);
        }

        fyllAnstalld();
    }                                               

    private void bLaggTillAnstalldActionPerformed(java.awt.event.ActionEvent evt) {                                                  
        String test = "";
        String fNamn = tbFnamn.getText();
        String eNamn = tbEnamn.getText();
        String mail = tbMail.getText();
        String telefon = tbTelefon.getText();
        String anvNamn = fNamn.substring(0,1).toLowerCase() + eNamn.substring(0,1).toLowerCase() +eNamn.substring(eNamn.length()-1,eNamn.length()).toLowerCase();

        try{
            String sqlFraga = "insert into ANSTALLD values ("+idb.getAutoIncrement("ANSTALLD", "AID")+",'"+fNamn+" "+eNamn+"','"+telefon+"','"+mail+"','"+anvNamn+"')";
            test = sqlFraga;
            idb.insert(sqlFraga);
            System.out.println("Det funkade " +sqlFraga);

        }

        catch (InfException e){
            System.out.println("Det funka INTE " +test);
        }

        fyllAnstalld();
    }                                                 

    private void cbSpecialistPopupMenuWillBecomeInvisible(javax.swing.event.PopupMenuEvent evt) {                                                          
        
        String getAID = getSpecAID();
       
        String sqlFragaSpecProjekt = "Select SPELPROJEKT.BETECKNING from SPELPROJEKT join ARBETAR_I on SPELPROJEKT.SID = ARBETAR_I.SID where ARBETAR_I.AID ="+getAID;
        String sqlFragaSpecDoman = "Select KOMPETENSDOMAN.BENAMNING from KOMPETENSDOMAN join HAR_KOMPETENS on KOMPETENSDOMAN.KID = HAR_KOMPETENS.KID where HAR_KOMPETENS.AID ="+getAID;
        String sqlFragaSpecPlatt = "Select PLATTFORM.BENAMNING from PLATTFORM join HAR_KOMPETENS on PLATTFORM.PID = HAR_KOMPETENS.PID where HAR_KOMPETENS.AID ="+getAID;
        
        
       // cbSpecialist.removeAllItems();
        listSpecDoman.removeAll();
        listSpecPlatt.removeAll();
        listSpecProj.removeAll();
        
        try{
            if(idb.fetchColumn(sqlFragaSpecProjekt) !=null)
            
            {
            ArrayList<String> specProjekt = idb.fetchColumn(sqlFragaSpecProjekt);
            System.out.println("Fungerar att fylla namn för projektet" + sqlFragaSpecProjekt);
         
            
            for(int i = 0; i<specProjekt.size();i++){
                listSpecProj.add(specProjekt.get(i));}
            
        }
            
            else {
                    listSpecProj.add("");
           
        }
        }
        
        catch (InfException e){
          
         System.out.println("Fungerar ej att fylla på projekt för denna anställda");
             
        }
        
        try{
            if(idb.fetchColumn(sqlFragaSpecDoman) != null){
            ArrayList<String> specDoman = idb.fetchColumn(sqlFragaSpecDoman);
            System.out.println("Fungerar att fylla namn för projektet" + sqlFragaSpecDoman);
            for(int i = 0; i<specDoman.size();i++){
                listSpecDoman.add(specDoman.get(i));
            }}
            
            else{
                listSpecDoman.add("");
            }
        }
        catch (InfException e){
             JOptionPane.showMessageDialog(null, e.getMessage());
            System.out.println("FAN!" + sqlFragaSpecDoman);
        }
        
        try{
            if(idb.fetchColumn(sqlFragaSpecPlatt) != null){
            ArrayList<String> specPlatt = idb.fetchColumn(sqlFragaSpecPlatt);
            System.out.println("Fungerar att fylla på plattformar för specialist" + sqlFragaSpecPlatt);
          
            for(int i=0;i<specPlatt.size();i++){
                listSpecPlatt.add(specPlatt.get(i));
            }
                 
                    }
            else {listSpecPlatt.add("");
        }
        }
            
           catch(InfException e){
               
           System.out.println("FUngerar ej att fylla på plattformar");
       
                    
                    
                    }
           
  
    }                                                         

    private void cbInfoProjMouseClicked(java.awt.event.MouseEvent evt) {                                        
     
        tfInfoProjBet.setText(cbInfoProj.getSelectedItem().toString());
        tfInfoProjStartdatum.setText(getStartdatumProj());
        tfInfoProjReleasedatum.setText(getReleasedatumProj());
        tfnyInfoBetcking.setText(cbInfoProj.getSelectedItem().toString());
        tfnyInfoStartdatum.setText(getStartdatumProj());
        tfnyInfoReleasedatum.setText(getReleasedatumProj());
        
    }                                       

    private void btnUppInfoProjActionPerformed(java.awt.event.ActionEvent evt) {                                               
        
        String projekt = cbInfoProj.getSelectedItem().toString();
        String nyttSd = tfnyInfoStartdatum.getText();
        String nyttRd = tfnyInfoReleasedatum.getText();
        String nyBet = tfnyInfoBetcking.getText();
        
        String sqlFragaNyttSd = "UPDATE SPELPROJEKT SET STARTDATUM = '"+nyttSd+"' WHERE BETECKNING = '"+projekt; 
        String sqlFragaNyttRd = "UPDATE SPELPROJEKT SET RELEASEDATUM = '"+nyttRd+"' WHERE BETECKNING = '"+projekt;
        String sqlFraganyBet = "UPDATE SPELPROJEKT SET BETECKNING = '"+nyBet+"' WHERE BETECKNING = '"+projekt;
        
        if(!tfnyInfoStartdatum.getText().isEmpty())
        {
            try{
                
                idb.update(sqlFragaNyttSd);
               
            }
            catch(InfException e)
            {
                System.out.println("Fungerar inte att byta datum");
                }
        }
        else if(!tfnyInfoBetcking.getText().isEmpty())
            try{
                idb.update(sqlFraganyBet);
                        
            }
        catch(InfException e)
        {
            System.out.println("FUngerar inte med nyBet");
        }
        
        else if(!tfnyInfoReleasedatum.getText().isEmpty())
                    {
                    try{
                    idb.update(sqlFragaNyttRd);
                            }
                    catch(InfException e)
                    {
                    System.out.println("Fungerar inte med RD");
                    }
                    
                    }
        
    }                                              

