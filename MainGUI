private String getProjekt(){
        String proj = "";
        String getProjCB = cbUppdProjPlattProj.getSelectedItem().toString();
        String getProjekt = "Select SID from SPELPROJEKT where beteckning = '" + getProjCB + "'";
        
        try {
             proj = idb.fetchSingle(getProjekt);
            System.out.println(getProjekt);

        }
        
        catch (InfException e){
            System.out.println("Fel mening //" + getProjekt);
        }
    
        return proj;
     }

private String getPlattform(){
        
        String platt = "";
        String getPlattCB = cbUppdProjPlattPlatt.getSelectedItem().toString();
        String sqlFraga = "SELECT PID FROM PLATTFORM WHERE BENAMNING= '" + getPlattCB + "'";
        
        try {
            
            platt = idb.fetchSingle(sqlFraga);
            System.out.println(sqlFraga);

        }
        
        catch (InfException e){
            System.out.println("Fel mening //" + sqlFraga);
        }
        
        return platt;
        
    }
