        tfAnstNamn2.setText("");
        tfAnvNamn2.setText("");
        tfNyMail.setText("");
        tfNyTlf.setText("");
        uppdateraGUI();
    }                                                    

    private void cbUppdAnstalldPopupMenuWillBecomeInvisible(javax.swing.event.PopupMenuEvent evt) {                                                            
        String namn = cbUppdAnstalld.getSelectedItem().toString();
        String aID = getAID(cbUppdAnstalld.getSelectedItem().toString());
        String tlf = getTlf(cbUppdAnstalld.getSelectedItem().toString());
        String mail = getMail(cbUppdAnstalld.getSelectedItem().toString());
        String anvNamn = getAnvnamn(cbUppdAnstalld.getSelectedItem().toString());
        
        tfAID1.setText(aID);
        tfAnstNamn1.setText(namn);
        tfGamTlf.setText(tlf);
        tfGamMail.setText(mail);
        tfAnvNamn1.setText(anvNamn);
        tfAID2.setText(aID);
        tfAnstNamn2.setText(namn);
        tfAnvNamn2.setText(anvNamn);
    }                                                           

    private void btnTaBortAnstalldActionPerformed(java.awt.event.ActionEvent evt) {                                                  
        String anstalldNamn = cbTaBortAnstalld.getSelectedItem().toString();
        String sqlFraga = "Delete from Anstalld where namn = '"+anstalldNamn+"'";
        
        try{
            idb.delete(sqlFraga);
        }
        
        catch (InfException e){
            System.out.println("Funkade inte " + sqlFraga);
        }
        
        fyllAnstalld();
    }                                                 

    private void btnLaggTillAnstalldActionPerformed(java.awt.event.ActionEvent evt) {                                                    
        
        String test = "";
        String fNamn = tfFnamn.getText();
        String eNamn = tfEnamn.getText();
        String mail = tfMail.getText();
        String telefon = tfTelefon.getText();
        
        if(ValidateInput.valideraText(fNamn))
        {
            String anvNamn = fNamn.substring(0,1).toLowerCase() + eNamn.substring(0,1).toLowerCase() +eNamn.substring(eNamn.length()-1,eNamn.length()).toLowerCase();
            
            
            String getAID = getAID(fNamn + " " + eNamn);
            
            try{
                
                String sqlFraga = "insert into ANSTALLD values ("+idb.getAutoIncrement("ANSTALLD", "AID")+",'"+fNamn+" "+eNamn+"','"+telefon+"','"+mail+"','"+anvNamn+"')";
                test = sqlFraga;
                idb.insert(sqlFraga);
                
            }
            
            catch (InfException e){
                System.out.println("Det funka INTE " +test);
            }
            
            
            
            
            fyllAnstalld();
        }
        else if(fNamn.length()<=0) {
            JOptionPane.showMessageDialog(null, "FUCK!");
            
        }
        uppdateraGUI();
    }                                                   

    private void cbSpecialistPopupMenuWillBecomeInvisible(javax.swing.event.PopupMenuEvent evt) {                                                          
        fillSpecialTable();
    }                                                         
    
    
    
    
    protected void fillSpecialTable(){
        
        DefaultTableModel model = (DefaultTableModel) tableSpecial.getModel();
        ArrayList<HashMap<String, String>> anstallda = null;
        model.setRowCount(0);
        
        String sqlFraga = "Select KOMPETENSDOMAN.BENAMNING as DOMAN, ANSTALLD.NAMN, PLATTFORM.BENAMNING, HAR_KOMPETENS.KOMPETENSNIVA\n" +
                "from HAR_KOMPETENS \n" +
                "join KOMPETENSDOMAN\n" +
                "on KOMPETENSDOMAN.KID = HAR_KOMPETENS.KID\n" +
                "join ANSTALLD on HAR_KOMPETENS.AID = ANSTALLD.AID \n" +
                "join PLATTFORM on PLATTFORM.PID = HAR_KOMPETENS.PID where ANSTALLD.NAMN = '"+cbSpecialist.getSelectedItem().toString()+"'";
        
        try {
            anstallda = idb.fetchRows(sqlFraga);
            System.out.println("funka");
        }
        catch (InfException e)
        {JOptionPane.showMessageDialog(null, e.getMessage());
        System.out.println("Fel");
        }
        for (int i = 0;i<anstallda.size();i++)
        {
            
            model.addRow(new Object[]{anstallda.get(i).get("DOMAN"),anstallda.get(i).get("BENAMNING"),anstallda.get(i).get("KOMPETENSNIVA")});
        }
        
    }
    protected void fyllDatumLista(){
        String sqlFragaForeDatum ="Select BETECKNING from SPELPROJEKT where releasedatum < current_date";
        String sqlFragaEfterDatum = "Select BETECKNING from SPELPROJEKT where releasedatum > current_date";
        
        listPasseratDatum.removeAll();
        listKommandeDatum.removeAll();
        
        try {
            ArrayList<String> kommandeDatum = idb.fetchColumn(sqlFragaForeDatum);
            System.out.println("Funkar att lägga till FÖREDATUM");
            for(int i = 0; i < kommandeDatum.size(); i++){
                listKommandeDatum.add(kommandeDatum.get(i));
            }
        }
        
        catch (InfException e){
            System.out.println("DET FUNKADE INTE ATT LÄGGA TILL FÖREDATUM");
            
        }
        
        try {
            ArrayList<String> passeratDatum = idb.fetchColumn(sqlFragaEfterDatum);
            System.out.println("Funkar att lägga till EFTERDATUM");
            for(int i = 0; i < passeratDatum.size(); i++){
                listPasseratDatum.add(passeratDatum.get(i));
            }
        }
        
        catch (InfException e){
            System.out.println("DET FUNKADE INTE ATT LÄGGA TILL EFTERDATUM");
            
        }
    }
    protected void fyllCBSpecialist(){
        
        String sqlFraga = "SELECT anstalld.Namn from anstalld join specialist on anstalld.aid = Specialist.aid";
        
        cbSpecialist.removeAllItems();
        
        
        try{
            
            ArrayList<String> projektID = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla värden för projekt");
            for(int i =0;i<projektID.size();i++)
            {
                cbSpecialist.addItem(projektID.get(i));
                
            }
        }
        catch(InfException e)
        {
            
            System.out.println("Fungerar ej att fylla projekt eller anställdl");
            
        }
    }
    protected void fyllCbProjekt(){
        
        String sqlFraga = "SELECT BETECKNING FROM SPELPROJEKT";
        
        cbProjID.removeAllItems();
        cbProjBet.removeAllItems();
        cbUppdProjPlattProj.removeAllItems();
        cbInfoProj.removeAllItems();
        cbListaProj.removeAllItems();
        
        
        try{
            
            ArrayList<String> projektID = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla värden för projekt");
            for(int i =0;i<projektID.size();i++)
            {
                cbProjID.addItem(projektID.get(i));
                cbProjBet.addItem(projektID.get(i));
                cbUppdProjPlattProj.addItem(projektID.get(i));
                cbInfoProj.addItem(projektID.get(i));
                cbListaProj.addItem(projektID.get(i));
            }
        }
        catch(InfException e)
        {
            
            System.out.println("Fungerar ej att fylla projekt eller anställda");
            
        }
        
    }
    protected void fyllAnstalld(){
        String sqlFraga = "SELECT NAMN  FROM ANSTALLD";
        
        cbUppAnProj.removeAllItems();
        cbTaBortAnstalld.removeAllItems();
        cbUppdAnstalld.removeAllItems();
        cbAnstDomPlatt.removeAllItems();
        cbProjLed.removeAllItems();
        cbProjledare.removeAllItems();
        
        try{
            ArrayList<String> anstallda = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla på anstallda");
            for(int i =0;i<anstallda.size();i++)
            {
                cbUppAnProj.addItem(anstallda.get(i));
                cbTaBortAnstalld.addItem(anstallda.get(i));
                cbUppdAnstalld.addItem(anstallda.get(i));
                cbAnstDomPlatt.addItem(anstallda.get(i));
            }
        }
        
        catch(InfException e)
        {
            
            System.out.println("Fel fel fel");
            
        }
        
        try{
            
            ArrayList<String> projektledare = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla cb med projektledare");
            for(int i =0;i<projektledare.size();i++)
            {
                cbProjledare.addItem(projektledare.get(i));
                cbProjLed.addItem(projektledare.get(i));
            }
        }
        catch(InfException e)
        {
            
            System.out.println("Fungerar ej att fylla cb med projektledare");
        }
    }
    
    private String getAID(String namn){
        String x = "";
        String sqlFraga = "Select AID from SPELPROJEKT where BETECKNING ='" +namn+"'";
        
        
        try{
            String y = idb.fetchSingle(sqlFraga);
            System.out.println("Det funkade "+sqlFraga);
            x=y;
        }
        
        catch (InfException e){
            System.out.println("Funkade inte "+sqlFraga);
        }
        
        return x;
        
    }
    private String getSID(String projektnamn){
        
        String proj = "";
        String sIDProjekt = projektnamn;
        String getProjekt = "Select SID from SPELPROJEKT where beteckning = '" + sIDProjekt + "'";
        
        try {
            proj = idb.fetchSingle(getProjekt);
            System.out.println(getProjekt);
            
        }
        
        catch (InfException e){
            System.out.println("Fel mening //" + getProjekt);
        }
        
        return proj;
    }
