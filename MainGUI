    }                                        

    private void btnLaggTillActionPerformed(java.awt.event.ActionEvent evt) {                                            
        
        String bet = tfBeteckning.getText();
        String startD = tfStartdatum.getText();
        String releaseD = tfReleasedatum.getText();
        String projektL = getAID(cbProjledare.getSelectedItem().toString());
        
        try{
            
            String sqlFraga = "INSERT INTO SPELPROJEKT VALUES("+idb.getAutoIncrement("SPELPROJEKT","SID")+",'"+bet+"','"+startD+"','"+releaseD+"',"+projektL+")";
            idb.insert(sqlFraga);
            
        }
        catch (InfException e)
        {
            System.out.println("Fungerar ej att fylla spelprojekt cb med värden ");
        }
        uppdateraGUI();
    }                                           

    private void btnUppInfoProjActionPerformed(java.awt.event.ActionEvent evt) {                                               
        
        String projekt = cbInfoProj.getSelectedItem().toString();
        String nyttSd = tfnyInfoStartdatum.getText();
        String nyttRd = tfnyInfoReleasedatum.getText();
        String nyBet = tfnyInfoBetcking.getText();
        
        String sqlFragaNyttSd = "UPDATE SPELPROJEKT SET STARTDATUM = '"+andraDatum(nyttSd)+"' WHERE BETECKNING = '"+projekt+"'";
        String sqlFragaNyttRd = "UPDATE SPELPROJEKT SET RELEASEDATUM = '"+andraDatum(nyttRd)+"' WHERE BETECKNING = '"+projekt+"'";
        String sqlFraganyBet = "UPDATE SPELPROJEKT SET BETECKNING = '"+nyBet+"' WHERE BETECKNING = '"+projekt+"'";
        
        try{
            
            idb.update(sqlFragaNyttSd);
            
        }
        catch(InfException e)
        {
            System.out.println("Fungerar inte att byta datum");
            
        }
        
        try{
            idb.update(sqlFraganyBet);
            
        }
        catch(InfException e)
        {
            System.out.println("FUngerar inte med nyBet");
            
        }
        
        try{
            idb.update(sqlFragaNyttRd);
            
            
        }
        catch(InfException e)
        {
            System.out.println("Fungerar inte med RD");
            
        }
        uppdateraGUI();

    }                                              

    private void cbInfoProjPopupMenuWillBecomeInvisible(javax.swing.event.PopupMenuEvent evt) {                                                        
        
        tfInfoProjBet.removeAll();
        tfInfoProjStartdatum.removeAll();
        tfInfoProjReleasedatum.removeAll();
        tfnyInfoBetcking.removeAll();
        tfnyInfoStartdatum.removeAll();
        tfnyInfoReleasedatum.removeAll();
        
        tfInfoProjBet.setText(cbInfoProj.getSelectedItem().toString());
        tfInfoProjStartdatum.setText(getStartdatum(cbInfoProj.getSelectedItem().toString()));
        tfInfoProjReleasedatum.setText(getReleasedatum(cbInfoProj.getSelectedItem().toString()));
        tfnyInfoBetcking.setText(cbInfoProj.getSelectedItem().toString());
        tfnyInfoStartdatum.setText(getStartdatum(cbInfoProj.getSelectedItem().toString()));
        tfnyInfoReleasedatum.setText(getReleasedatum(cbInfoProj.getSelectedItem().toString()));
    }                                                       

