{
    String startDatum = "";
    String sqlFraga = "SELECT STARTDATUM FROM SPELPROJEKT WHERE BETECKNING = '"+cbInfoProj.getSelectedItem().toString()+"'";
    
    try{
        
        startDatum = idb.fetchSingle(sqlFraga);
        
    }
    catch(InfException e)
    {
        System.out.println("Fungerar ej att få tag i datum från spelprojekt");
    
    }

    return startDatum;
}

