    private void uppdateraGUI()
    {
        
        lblError.setText(errorText);
        fyllCbProjekt();
        
        fyllAnstalld();
        fyllCBSpecialist();
        fyllPlattform();
        fyllDoman();
        
        fyllDatumLista();
        
    }
    static  public void setErrorText(String string)
    {
        errorText = string;
    }
    
    
    private void cbListaProjPopupMenuWillBecomeInvisible(javax.swing.event.PopupMenuEvent evt) {                                                         
        
        String AIDProjLedare = getAID(cbListaProj.getSelectedItem().toString());
        String SID = getSID(cbListaProj.getSelectedItem().toString());
        
        String sqlFragaNamn = "Select Namn from ANSTALLD where AID ="+AIDProjLedare;
        String sqlFragaTlf = "Select Telefon from ANSTALLD where AID = " +AIDProjLedare;
        String sqlFragaMail = "Select Mail from ANSTALLD where AID = " +AIDProjLedare;
        String sqlFragaanvnamn = "Select anvnamn from ANSTALLD where AID = " +AIDProjLedare;
        String sqlFragaAnstallda = "Select ANSTALLD.NAMN from ANSTALLD JOIN ARBETAR_I ON ANSTALLD.AID = ARBETAR_I.AID WHERE SID =" +SID;
        String sqlFragaPlattform = "Select PLATTFORM.BENAMNING from PLATTFORM join INNEFATTAR on PLATTFORM.PID = INNEFATTAR.PID where SID =" +SID;
        
        listProjLedare.removeAll();
        listAnstalld.removeAll();
        listPlattform.removeAll();
        
        try{
            ArrayList<String> PlattformProjekt = idb.fetchColumn(sqlFragaPlattform);
            for(int i = 0; i<PlattformProjekt.size();i++){
                listPlattform.add(PlattformProjekt.get(i));
            }
        }
        
        catch (InfException e){
            System.out.println("Det funkade inte");
        }
        
        try{
            ArrayList<String> AnstalldaProjekt = idb.fetchColumn(sqlFragaAnstallda);
            for(int i = 0; i<AnstalldaProjekt.size();i++){
                listAnstalld.add(AnstalldaProjekt.get(i));
            }
        }
        
        catch (InfException e){
            System.out.println("Det funkade inte");
        }
        
        try{
            String x = idb.fetchSingle(sqlFragaNamn);
            listProjLedare.add(x);
            
        }
        
        catch (InfException e){
            System.out.println("Det funkade inte");
        }
        try{
            String x = idb.fetchSingle(sqlFragaTlf);
            listProjLedare.add(x);
            
        }
        
        catch (InfException e){
            System.out.println("Det funkade inte");
        }
        try{
            String x = idb.fetchSingle(sqlFragaMail);
            listProjLedare.add(x);
            
        }
        
        catch (InfException e){
            System.out.println("Det funkade inte");
        }
        try{
            String x = idb.fetchSingle(sqlFragaanvnamn);
            listProjLedare.add(x);
            
        }
        
        catch (InfException e){
            System.out.println("Det funkade inte");
        }
    }                                                        

    private void btnUppdateraDomanActionPerformed(java.awt.event.ActionEvent evt) {                                                  
        String andraNamn = tfNyttNamn.getText();
        String andraKomm = taAndraKomm.getText();
        String gammaltNamn = cbDoman.getSelectedItem().toString();
        
        String sqlFragaNamn = "Update KOMPETENSDOMAN set BENAMNING ='"+andraNamn+"' where BENAMNING='" +gammaltNamn+"'";
        String sqlFragaKomm = "Update KOMPETENSDOMAN set BESKRIVNING  = '"+andraKomm+"' where BENAMNING='" +andraNamn+"'";
        
        try {
            idb.update(sqlFragaNamn);
            
        }
        
        catch (InfException e){
            System.out.println("Det vart fel");
            System.out.println(sqlFragaNamn);
        }
        
        try {
            idb.update(sqlFragaKomm);
            
        }
        
        catch (InfException e){
            System.out.println("Det vart fel");
            System.out.println(sqlFragaKomm);
        }
        uppdateraGUI();
        
    }                                                 

