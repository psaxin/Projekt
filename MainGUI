private String getSpecAID(){
    String x = "";
    String specAID = cbSpecialist.getSelectedItem().toString();
    String sqlFraga = "Select AID from Anstalld where Namn ='" +specAID+"'";
    
    try{
        String y = idb.fetchSingle(sqlFraga);
        System.out.println("Det funkade!! " + y);
        x=y;
    }
    
    catch (InfException e) {
        System.out.println("Det funkade inte " +sqlFraga);
    }
    
    return x;
}    
private String  getAid() {
        String y = "";
        
         String projekt = cbProjledare.getSelectedItem().toString();
          String sqlFraga = "SELECT AID FROM ANSTALLD WHERE NAMN = '"+projekt+"'";
          try{
              
              y = idb.fetchSingle(sqlFraga);
              System.out.println("getAid Funka");
          }
          catch(InfException e)
          {
              System.out.println("Funka inte");
          }
          
          return y;
 }
    
protected  void fyllCbProjekledare() {
        
         String sqlFraga =   "SELECT ANSTALLD.NAMN FROM ANSTALLD JOIN PROJEKTLEDARE  ON ANSTALLD.AID = PROJEKTLEDARE.AID ";
        
        cbProjledare.removeAllItems();
        cbUPL.removeAllItems();
        
        try{
            
                ArrayList<String> projektledare = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla cb med projektledare");
            for(int i =0;i<projektledare.size();i++)
            {
                cbProjledare.addItem(projektledare.get(i));
                cbUPL.addItem(projektledare.get(i));
            }
        }
        catch(InfException e)
                    {    
                       
                System.out.println("Fungerar ej att fylla cb med projektledare");
                    }
        
      
    }

protected void fyllCBSpecialist(){
    
        String sqlFraga = "SELECT anstalld.Namn from anstalld join specialist on anstalld.aid = Specialist.aid";
        
         cbSpecialist.removeAllItems();

    
          try{

            ArrayList<String> projektID = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla värden för projekt");
            for(int i =0;i<projektID.size();i++)
            {
                cbSpecialist.addItem(projektID.get(i));
                
            }
          }
           catch(InfException e)
        {

            System.out.println("Fungerar ej att fylla projekt eller anställdl");

        }
}
 
protected void fyllCbProjekt(){
        
        String sqlFraga = "SELECT BETECKNING FROM SPELPROJEKT";
        
         cbUPID.removeAllItems();
         cbLaggTillAnsProj.removeAllItems();
         cbUppdProjPlattProj.removeAllItems();
         cbInfoProj.removeAllItems();
         cbProjLedare.removeAllItems();

    
          try{

            ArrayList<String> projektID = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla värden för projekt");
            for(int i =0;i<projektID.size();i++)
            {
                cbUPID.addItem(projektID.get(i));
                cbLaggTillAnsProj.addItem(projektID.get(i));
                cbUppdProjPlattProj.addItem(projektID.get(i));
                cbInfoProj.addItem(projektID.get(i));
                cbProjLedare.addItem(projektID.get(i));
            }
          }
           catch(InfException e)
        {

            System.out.println("Fungerar ej att fylla projekt eller anställdl");

        }
          
    }
    
protected void fyllAnstalld(){
         String sqlFraga = "SELECT NAMN  FROM ANSTALLD";
         
         cbUppAnProj.removeAllItems();
         cbTaBortAnstalld.removeAllItems();
         cbUppdAnstalld.removeAllItems();
         cbDoPlAnstalld.removeAllItems();
          
         try{
         ArrayList<String> anstallda = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla på anstallda");
            for(int i =0;i<anstallda.size();i++)
            {
                cbUppAnProj.addItem(anstallda.get(i));
                cbTaBortAnstalld.addItem(anstallda.get(i));
                cbUppdAnstalld.addItem(anstallda.get(i));
                cbDoPlAnstalld.addItem(anstallda.get(i));
            }
        }

        catch(InfException e)
        {

            System.out.println("Fel fel fel");
            
        }
    }
    
protected void fyllDoman(){
        String sqlFraga = "Select BENAMNING FROM KOMPETENSDOMAN";
        cbDoman.removeAllItems();
        cbDoPlDoman.removeAllItems();
        
        try{
            ArrayList<String> domaner = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla domäner");
            for(int i = 0; i<domaner.size();i++){
                cbDoman.addItem(domaner.get(i));
                cbDoPlDoman.addItem(domaner.get(i));
            }
        }
        
        catch (InfException e){
            System.out.println("Det fungerade inte att lägga till domäner");
        }
    }
protected void fyllPlattform(){
         String sqlFraga = "SELECT BENAMNING FROM PLATTFORM";
        cbUppdProjPlattPlatt.removeAllItems();
        cbDoPlPlattform.removeAllItems();
       
         try{
            
              ArrayList<String> plattformar = idb.fetchColumn(sqlFraga);
            System.out.println("Fungerar att fylla på plattformar");
            for(int i =0;i<plattformar.size();i++)
            {
                cbUppdProjPlattPlatt.addItem(plattformar.get(i));
                cbDoPlPlattform.addItem(plattformar.get(i));
            }
         }
         
          catch(InfException e)
        {
            System.out.println("Fungerade EJ");
        
        }
  }
